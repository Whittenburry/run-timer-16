<!-- Listing out all recorded times-->

{{outlet}}

<div class="time-container-container-container-container">
  <div class="time-container">
    <div class="time-container__date">
      <h3 class="time-container__date-item">Nov 4, 2015</h3>
    </div>
    <div class="time-container__time">
      <h3 class="time-container__time-item">23:55:74</h3>
    </div>
  </div>
</div>

<footer class="footer">
{{#link-to "new"}}
  <button class="fa fa-plus add-new"></button>
{{/link-to}}
</footer>


