{{outlet}}

<header class="description-header">
  <h1 class="description-header__title">My Run</h1>
</header>

<div class="run-description-container">
  <form class="run-description-form">
    <div class="run-info">
      {{input class="run-info__field description-date" type="text" value=runTime}}
      {{input class="run-info__field description-time" type="text" value=runDate}}
    </div>
    <div class="run-info">
      {{input class="run-info__description" type="text" value=raceNote}}
    </div>
  </form>
</div>
